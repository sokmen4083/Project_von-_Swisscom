{
  "_version": "1.32.0",
  "sap.app": {
    "id": "com.swisscom.socialbee.plan.resourceplan",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZPLAPRJBER_CDS_CDS/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.99.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.f": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.swisscom.socialbee.plan.resourceplan.i18n.i18n"
        }
      },
      "ProjPlanODataModel": {
        "dataSource": "mainService",
        "settings": {
					"useBatch": false,
					"metadataUrlParams": {
						"sap-documentation": "heading"
					}
				}
      },
      "ProjPlanJsonModel": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "EmployeeJsonModel": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "ProjectJsonModel": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "KundeJsonModel": {
        "type": "sap.ui.model.json.JSONModel"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "com.swisscom.socialbee.plan.resourceplan.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "bypassed": {
          "target": [
            "notFound"
          ]
        },
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "home",
          "target": [
            "home"
          ]
        },
        {
          "pattern": "ma",
          "name": "mitarbeiterList",
          "target": [
            "mitarbeiterList"
          ]
        },
        {
          "pattern": "kunde",
          "name": "kundeList",
          "target": [
            "kundeList"
          ]
        },
        {
          "pattern": "wl",
          "name": "projectList",
          "target": [
            "projectList"
          ]
        },
        {
          "pattern": "ZPLAPRJBER_CDS/{objectId}",
          "name": "projectDetail",
          "target": [
            "projectDetail"
          ]
        },
        {
          "pattern": "ZPLANMITARBEITER_CDS/{objectId}",
          "name": "mitarbeiterDetail",
          "target": [
            "mitarbeiterDetail"
          ]
        },
        {
          "pattern": "ZPLAKUNDE_CDS/{objectId}",
          "name": "kundeDetail",
          "target": [
            "kundeDetail"
          ]
        }
      ],
      "targets": {
        "home": {
          "viewName": "Home",
          "viewId": "home",
          "viewLevel": 1,
          "title": "Home"
        },
        "mitarbeiterList": {
          "viewName": "MitarbeiterList",
          "viewId": "mitarbeiterList",
          "viewLevel": 1,
          "title": "MitarbeiterList"
        },
        "kundeList": {
          "viewName": "KundeList",
          "viewId": "kundeList",
          "viewLevel": 1,
          "title": "KundeList"
        },
        "projectList": {
          "viewName": "ProjectList",
          "viewId": "projectList",
          "viewLevel": 1,
          "title": "ProjectList"
        },
        "projectDetail": {
          "viewName": "ProjectDetail",
          "viewId": "projectDetail",
          "viewLevel": 2,
          "title": "{i18n>objectViewTitle}"
        },
        "mitarbeiterDetail": {
          "viewName": "MitarbeiterDetail",
          "viewId": "mitarbeiterDetail",
          "viewLevel": 2,
          "title": "{i18n>objectViewTitle}"
        },
        "kundeDetail": {
          "viewName": "KundeDetail",
          "viewId": "kundeDetail",
          "viewLevel": 2,
          "title": "{i18n>objectViewTitle}"
        },
        "objectNotFound": {
          "viewName": "ObjectNotFound",
          "viewId": "objectNotFound"
        },
        "notFound": {
          "viewName": "NotFound",
          "viewId": "notFound"
        }
      }
    },
    "rootView": {
      "viewName": "com.swisscom.socialbee.plan.resourceplan.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    }
  }
}
